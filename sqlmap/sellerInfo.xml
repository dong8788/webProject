<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="sellerInfoDao">
		
		
		<select resultType="SellerInfoVo" id="selelctAll"> 
			select * from SELLER order by SELLERSEQ
		</select>
			
		<select resultType="SellerinfoVo" id="selectOne">
			select * from SELLER where SELLERSEQ = #{seq}
		</select>	






</mapper>




